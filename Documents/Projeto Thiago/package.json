{
  "name": "prosperty-brazil",
  "version": "1.0.0",
  "description": "Prosperty Brazil - Real Estate Technology Platform for CAIXA and B2C Market",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "tsc",
    "start": "npx prisma migrate deploy && node dist/server.js",
    "postinstall": "npx prisma generate",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "real-estate",
    "property",
    "auction",
    "crm",
    "avm",
    "brazil"
  ],
  "author": "Prosperty Brazil",
  "license": "MIT",
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "multer": "^2.0.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0"
  }
}


